{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Data Protection Enforcement Case",
  "description": "A representation of a Data Protection Enforcement case of the Personal Data Protection Commission in Singapore",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "citation": {
      "type": "string"
    },
    "commissioner": {
      "type": "object",
      "properties": {
        "name": {"type": "string"},
        "designation": {"type": "string"}
      }
    },
    "summary": {
      "type": "string"
    },
    "respondent": {
      "type": "object",
      "properties": {
        "name": {"type": "string"}
      }
    }
  }
}